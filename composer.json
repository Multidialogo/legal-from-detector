{
  "name": "multidialogo/legal-form-guesser",
  "description": "Legal form guesser: guess a company legal form from its extended name",
  "keywords": [
    "php",
    "legal form guesser",
    "company acronyms"
  ],
  "require": {
    "php": "^8.2"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "multidialogo\\LegalFormGuesser\\": "src/"
    }
  },
  "autoload-dev": {
    "files": ["test/autoload.php"],
    "psr-4": {
      "multidialogo\\test\\LegalFormGuesser\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  }
}
